    "query": {
        "from": {
            "table": "Feeds\\Articles\\Code\\Tables\\Articles",
            "alias": "fa",
            "name": "#__feeds_articles"
        },
        "select": [
            "fa.id",
            "fa.category_id",
            "fc.title AS category_id_title",
            "fa.feed_id",
            "fc_493.title AS feed_id_title",
            "fa.title",
            "fa.url",
            "fa.image",
            "mm.title AS image_title",
            "mm.file AS image_file",
            "fa.content",
            "fa.published",
            "fa.created_by",
            "fa.date_created",
            "fa.modified_by",
            "fa.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Feeds\\Categories\\Code\\Tables\\Categories",
                "alias": "fc",
                "name": "#__feeds_categories",
                "on": "fa.category_id = fc.id"
            },
            {
                "table": "Feeds\\Categories\\Code\\Tables\\Categories",
                "alias": "fc_493",
                "name": "#__feeds_categories",
                "on": "fa.feed_id = fc_493.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "fa.image = mm.id"
            }
        ]
    }
